QTabBar {

}

QTabBar::tab {
    border-top-right-radius: 10px;
    border-top-left-radius: 6px;
    width: 128px;
    min-height: 19px;
    max-height: 19px;
    min-width: 8ex;
    margin-top: 0px;
}

QTabBar::tab:hover {

}

QTabBar::tab:selected {
    margin-top: -4px;
}

QTabBar::tab:!selected {
    margin-top: 2px;
}
